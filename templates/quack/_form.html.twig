{{ form_start(form) }}
<div class="formLabelWidgetQuack">
    <div>
        {{ form_label(form.content) }}
    </div>
    <div class="">
        {{ form_widget (form.content) }}
    </div>
</div>
<div class="formLabelWidgetQuack">
    <div>
        {{ form_label(form.image) }}
    </div>
    <div class="formImage">
        {{ form_widget (form.image) }}
    </div>
</div>
<div class="buttonCenter">
    <button class="profileButton greenButton">{{ button_label|default('Save') }}</button>
</div>
{{ form_end(form) }}


